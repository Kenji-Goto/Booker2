<h2>New Book</h2>
  <%= form_with model:@book ,url:books_path do |f|%>
    <div class="form-group">
      Title</br>
      <%= f.text_field :title%>
    </div>
    <div class="form-group">
      Opinion</br>
      <%= f.text_area :body%>
    </div>
    <%= f.submit"Create book",class:"btn btn-success"%>
    <% end %>
</div>